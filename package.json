{
  "name": "new-nodejs-app",
  "version": "1.0.0",
  "description": "NodeJs REST API",
  "main": "app.js",
  "scripts": {
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn dev-server\" \"yarn client\"",
    "clean:dist": "rm -rf dist && mkdir dist",
    "build-server": "babel -d ./dist ./src",
    "build": "npm run clean:dist && npm run build-server",
    "start": "NODE_ENV=development nodemon --exec babel-node ./src/app.js"
  },
  "keywords": [
    "NodeJS",
    "generate-node-project"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^1.0.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "nodemon": "^2.0.0"
  }
}
